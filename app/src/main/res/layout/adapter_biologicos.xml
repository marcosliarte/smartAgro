<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <androidx.cardview.widget.CardView
        android:id="@+id/card1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardBackgroundColor="@color/roxo_fundo_card"
        app:cardCornerRadius="8dp"
        app:cardElevation="4dp"
        app:cardUseCompatPadding="true">

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:cardBackgroundColor="@color/roxo_fundo_card2"
            app:cardCornerRadius="8dp"
            app:cardElevation="4dp"
            app:cardUseCompatPadding="true">

            <View
                android:layout_width="match_parent"
                android:layout_height="10dp"
                android:background="@color/view"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/registro_produto_textview" />

            <TextView
                android:id="@+id/registro_produto_textview"
                android:layout_width="200dp"
                android:layout_height="22dp"
                android:layout_marginLeft="180dp"
                android:layout_marginTop="20dp"
                android:text="registro_produto_textview"
                android:textColor="@color/text_cor"
                android:textSize="16sp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="116dp"
                android:layout_marginTop="20dp"
                android:text="Registro "
                android:textColor="@color/text_cor"
                android:textSize="16sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/marca_comercial_textview"
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="138dp"
                android:layout_marginTop="50dp"
                android:text="marca_comercial_textview"
                android:textColor="@color/text_cor"
                android:textSize="16sp" />


            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginTop="50dp"
                android:text="Marca comercial:"
                android:textColor="@color/text_cor"
                android:textSize="16sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/titular_registro_textview"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="147dp"
                android:layout_marginTop="85dp"
                android:textColor="@color/text_cor"
                android:text="titular_registro_textview"
                android:textSize="16sp" />

            <TextView
                android:layout_width="144dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginTop="85dp"
                android:text="Titular do registro:"
                android:textColor="@color/text_cor"
                android:textSize="16sp"
                android:textStyle="bold" />

            <TextView
                android:layout_width="96dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginTop="150dp"
                android:text="Ingredientes:"
                android:textColor="@color/text_cor"
                android:textSize="16sp"
                android:textStyle="bold" />

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="85dp"
                android:layout_marginStart="5dp"
                android:layout_marginEnd="5dp"
                android:layout_marginTop="168dp"
                app:cardBackgroundColor="@color/fundo_classes"
                app:cardElevation="4dp"
                app:cardUseCompatPadding="true">

                <TextView
                    android:id="@+id/ingrediente_ativo_textview"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="8dp"
                    android:text="ingrediente_ativo_textview"
                    android:textColor="@color/text_cor"
                    android:textSize="16sp" />
            </androidx.cardview.widget.CardView>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginTop="120dp"
                android:textColor="@color/text_cor"
                android:text="Aprovado para agricultura organica:"
                android:textSize="16sp"
                android:textStyle="bold" />

            <androidx.cardview.widget.CardView
                android:layout_width="70dp"
                android:layout_height="30dp"
                android:layout_marginLeft="269dp"
                android:layout_marginTop="118dp"
                app:cardBackgroundColor="@color/verde_view"
                app:cardCornerRadius="8dp"
                app:cardUseCompatPadding="true">

                <TextView
                    android:id="@+id/aprovado_agricultura_organica_textview"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="17dp"
                    android:layout_marginTop="-3dp"
                    android:text="boolean"
                    android:textColor="@color/white"
                    android:textSize="16sp" />
            </androidx.cardview.widget.CardView>

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="265dp"
            app:cardBackgroundColor="@color/roxo_fundo_card2"
            app:cardCornerRadius="8dp"
            app:cardUseCompatPadding="true">

            <View
                android:layout_width="match_parent"
                android:layout_height="10dp"
                android:background="@color/view"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/registro_produto_textview" />

            <TextView
                android:id="@+id/classificacao_toxicologica_textview"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginTop="35dp"
                android:text="classificacao_toxicologica_textview"
                android:textColor="@color/text_cor"
                android:textSize="16sp" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginTop="15dp"
                android:text="Classificação toxicologica"
                android:textColor="@color/text_cor"
                android:textSize="16sp"
                android:textStyle="bold" />


            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginTop="75dp"
                android:text="Classificação ambiental"
                android:textColor="@color/text_cor"
                android:textSize="16sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/classificacao_ambiental_textview"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/text_cor"
                android:layout_marginStart="10dp"
                android:layout_marginTop="95dp"
                android:text="classificacao_ambiental_textview"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/url_textview"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="159dp"
                android:layout_marginTop="140dp"
                android:text="url_textview"
                android:textColor="@color/text_cor"
                android:textStyle="bold"

                android:textSize="16sp" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="120dp"
                android:layout_marginTop="140dp"
                android:text="URL:"
                android:textStyle="bold"
                android:textColor="@color/text_cor"
                android:textSize="16sp" />

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="160dp"
                app:cardBackgroundColor="@color/fundo_classes"
                app:cardCornerRadius="4dp"
                app:cardElevation="4dp"
                app:cardUseCompatPadding="true">


                <TextView
                    android:layout_width="65dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:text="Classes:"
                    android:textColor="@color/text_cor"
                    android:textSize="16sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/classes_textview"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="74dp"
                    android:text="classes_textview:"
                    android:textColor="@color/text_cor"
                    android:textSize="16sp" />

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="250dp"
                app:cardBackgroundColor="@color/fundo_classes"
                app:cardCornerRadius="8dp"
                app:cardUseCompatPadding="true">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:text="Pragas:"
                    android:textColor="@color/text_cor"
                    android:textSize="16sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/pragas_text"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="68dp"
                    android:textColor="@color/text_cor"
                    android:text="pragas_text"
                    android:textSize="16sp" />
            </androidx.cardview.widget.CardView>


        </androidx.cardview.widget.CardView>
    </androidx.cardview.widget.CardView>
</LinearLayout>